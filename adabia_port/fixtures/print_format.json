  "html": "{% set application_specialist = 'اخصائي التطبيقات' %}\r\n{% set project_manager = 'مدير المشروع' %}\r\n{% set res_elec_department = 'مسئول الإدارة الإليكترونية' %}\r\n{% set responsible = 'مسئول '%}\r\n{% set user = frappe.db.get_value(\"User\", frappe.session.user, \"full_name\") %}\r\n{% set list = []  %}\r\n{% set E_admin_officer = frappe.db.get_value(\"User\", {\"job_title\" : \"E-Administration Officer\"} , \"full_name\") %}\r\n{% set sps_project_manager = frappe.db.get_value(\"User\", {\"job_title\" : \"SPS Project Manager\"} , \"full_name\") %}\r\n\r\n{%- for mod in doc.modules -%}\r\n    {% set _ = list.append(frappe.db.get_value('SPS Module', mod.module, 'module_name')) %}\r\n{% endfor %}\r\n\r\n<!--<div style=\"print-format-header\">-->\r\n<!--    <img src=\"/files/SPS_Header_LOGOe6406c1b0682.jpg\" alt=\"ISFP Header\" style=\"width: 100%;\">-->\r\n<!--</div>-->\r\n\r\n    <table class=\"table table-striped table-bordered text-center\">\r\n      <thead>\r\n        <tr >\r\n          <th class='text-center'>Job Description</th>\r\n          <th class='text-center'>Approved By</th>\r\n          <th class='text-center'>Reviewed By</th>\r\n          <th class='text-center'>Prepared By</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr>\r\n          <td class=\"\">{{ application_specialist }}</td>\r\n          <td class=\"text-success\"></td>\r\n          <td></td>\r\n          <td>{{ user }}</td><!---->\r\n        </tr>\r\n        <tr>\r\n          <td class=\"\">{{ project_manager }}</td>\r\n          <td class=\"text-success\">APPROVED</td>\r\n          <td>{{sps_project_manager}}</td>\r\n          <td>-</td>\r\n        </tr>\r\n        <tr>\r\n          <td class=\"\">{{ res_elec_department }}</td>\r\n          <td class=\"text-success\">APPROVED</td>\r\n          <td>{{ E_admin_officer }}</td>\r\n          <td> </td>\r\n        </tr>\r\n        {% for item in list %}\r\n            <tr>\r\n                <td class=\"\"> {{responsible}}{{ item }}</td>\r\n                <td class=\"text-success\">APPROVED</td>\r\n                <td>{{frappe.db.get_value(\"User\", {\"job_title\" : responsible + item } , \"full_name\")}}</td>\r\n                <td></td>\r\n            </tr>\r\n        {% endfor %}\r\n        \r\n      </tbody>\r\n    </table>\r\n        <hr/>\r\n        <br><br>\r\n        <br><br>\r\n        <br><br>\r\n        <table class=\"table table-striped text-start\">\r\n        <tbody>\r\n            <tr class='text-left'>\r\n                <td>{{doc.name}}</td>\r\n                <th scope=\"col\">Ticket #</th>\r\n              \r\n            </tr>\r\n            <tr>\r\n                <td>SC-ZPNE ADABIA PORT</td>\r\n                <th scope=\"col\">Project</th>\r\n              \r\n            </tr>\r\n            <tr>\r\n                <td>{{doc.requestor}}</td>\r\n                <th scope=\"col\">Requestor(s)</th>\r\n              \r\n            </tr>\r\n            <tr>\r\n                <td>{{doc.title}}</td>\r\n                <th scope=\"col\">Title</th>\r\n              \r\n            </tr>\r\n            <tr>\r\n                <td>{{frappe.db.get_value(\"User\", frappe.session.user, \"full_name\")}}</td>\r\n                <th scope=\"col\">Analyst(s)</th>\r\n              \r\n            </tr>\r\n            <tr>\r\n                <td>{{doc.details}}</td>\r\n                <th scope=\"col\">Details</th>\r\n              \r\n            </tr>\r\n        </tbody>\r\n    </table> \r\n    \r\n    <div class=\"page-break\"></div>\r\n\r\n\r\n{% if doc.scope %}\r\n    <div>\r\n        {{doc.scopes}}\r\n    </div>\r\n{% endif %}\r\n\r\n{% if doc.requirments %}\r\n<div>\r\n    {{doc.requirments}}\r\n</div>\r\n{% endif %}\r\n\r\n{% if doc.description %}\r\n    <div>\r\n    {{doc.description}}\r\n</div>\r\n{% endif %}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n",
